{
  "name": "fe-tools utils",
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./packages/utils/src/*.ts",
    "./packages/node-utils/src/index.ts",
    "./packages/web-utils/src/*.ts",
    "./packages/canvas-utils/src/index.ts",
    "./packages/ai-utils/src/index.ts",
    "./packages/node-img-build/src/handleImg.ts"
  ],
  "tsconfig": "./tsconfig.json",
  // "entryPointStrategy": "packages",
  "out": "../docs",
  "disableSources": false,
  "exclude": ["*.mock.ts", "*.test.ts"],
  "includeVersion": true,

  "plugin": ["typedoc-plugin-extras", "typedoc-theme-yaf"],
  "versions": {
    "stable": "1.0.0-beta01",
    "dev": "1.0.0-beta01"
  },

  "theme": "yaf",

  // extra config
  "footerLastModified": true
}
