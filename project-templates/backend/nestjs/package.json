{
    "name": "nestjs-template",
    "version": "1.0.0",
    "port": "5000",
    "author": "Wayne",
    "description": "nestjs nodejs server template.",
    "main": "dist/main.js",
    "scripts": {
        "start": "tsc && node dist/main.js",
        "dev": "tsc && set NODE_ENV=development && node dist/main.js",
        "build": "rm -rf dist/* && tsc",
        "build:prod": "tsc && rm -rf node_modules && npm install --production",
        "install:prod": "npm install --production",
        "docker:start": "npm install && node dist/main.js"
    },
    "keywords": [
        "nestjs",
        "nodejs"
    ],
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^18.0.0",
        "typescript": "^5.6.0"
    },
    "dependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",

        "@nestjs/platform-fastify": "^10.4.20",
        "fastify": "^4.29.1",
        "log4js": "^6.9.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.0"
    }
}